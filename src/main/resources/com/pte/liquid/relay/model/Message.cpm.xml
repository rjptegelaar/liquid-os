<?xml version="1.0"?><!DOCTYPE compass-core-mapping PUBLIC  
  "-//Compass/Compass Core Mapping DTD 2.2//EN"  
  "http://www.compass-project.org/dtd/compass-core-mapping-2.2.dtd">
<compass-core-mapping package="com.pte.liquid.relay.model">
 
    <class name="Message" alias="message"  root="true">
     	<id name="id" />
     	<constant>
            <meta-data>type</meta-data>
            <meta-data-value>message</meta-data-value>
        </constant>
        <property name="location">
            <meta-data>location</meta-data>
        </property>
        <property name="snapshotTime">
            <meta-data>snapshotTime</meta-data>
        </property>
        <property name="snapshotTimeMillis">
            <meta-data>snapshotTimeMillis</meta-data>
        </property>
        <component name="headers" ref-alias="messageHeader"/>   	
        <component name="parts" ref-alias="messagePart"/>
        <property name="correlationID">
            <meta-data>correlationID</meta-data>
        </property>  
        <property name="parentID">
            <meta-data>parentID</meta-data>
        </property>
        <property name="order">
            <meta-data>order</meta-data>
        </property>                                          
    </class>

</compass-core-mapping>